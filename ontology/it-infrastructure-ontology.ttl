# Property Chains for Cross-Layer Traversal
# ----------------------------------------------------------------------------

# Property chain: Application to Physical Infrastructure through Container
[]  rdf:type owl:Axiom ;
    owl:annotatedSource :Application ;
    owl:annotatedProperty :runs_on ;
    owl:annotatedTarget :PhysicalServer ;
    rdfs:comment "Application runs on physical infrastructure through container deployment" ;
    owl:propertyChainAxiom ( :deployed_as :runs_on ) .

# Property chain: Business Process to Application to Infrastructure
[]  rdf:type owl:Axiom ;
    owl:annotatedSource :BusinessProcess ;
    owl:annotatedProperty :depends_on_infrastructure ;
    owl:annotatedTarget :PhysicalInfrastructureLayer ;
    rdfs:comment "Business process depends on infrastructure through application realization" ;
    owl:propertyChainAxiom ( :realized_by :hosted_on ) .

# Property chain: Application to Storage through Database
[]  rdf:type owl:Axiom ;
    owl:annotatedSource :Application ;
    owl:annotatedProperty :uses_storage ;
    owl:annotatedTarget :StorageVolume ;
    rdfs:comment "Application uses storage through database" ;
    owl:propertyChainAxiom ( :uses :stored_on ) .

