  :security_type "firewall" ;
  :firewall_type "next_gen" ;
  :location "Datacenter-Chicago" ;
  :management_ip "10.2.0.3" ;
  :lifecycle_status "active" .

inst:CloudSecurityGroup03
  rdf:type :Firewall ;
  :name "sg-analytics-service" ;
  :description "Security group for analytics service" ;
  :security_type "firewall" ;
  :firewall_type "cloud" ;
  :location "us-west-2" ;
  :lifecycle_status "active" .

inst:HybridCertificate
  rdf:type :Certificate ;
  :name "*.inventory.example.com" ;
  :description "Wildcard certificate for inventory services" ;
  :security_type "certificate" ;
  :certificate_type "ssl_tls" ;
  :subject "CN=*.inventory.example.com, O=Example Corp, C=US" ;
  :issuer "CN=DigiCert Global CA, O=DigiCert Inc, C=US" ;
  :common_name "*.inventory.example.com" ;
  :valid_from "2024-01-01T00:00:00Z"^^xsd:dateTime ;
  :valid_to "2026-01-01T00:00:00Z"^^xsd:dateTime ;
  :expiration_date "2026-01-01T00:00:00Z"^^xsd:dateTime ;
  :lifecycle_status "active" .

inst:DigiCertCA
  rdf:type :CertificateAuthority ;
  :name "DigiCert Global CA" ;
  :description "DigiCert certificate authority" ;
  :security_type "certificate" ;
  :ca_type "root_ca" ;
  :subject "CN=DigiCert Global CA, O=DigiCert Inc, C=US" ;
  :issuer "CN=DigiCert Global Root CA, O=DigiCert Inc, C=US" ;
  :trust_level "trusted" ;
  :lifecycle_status "active" .

inst:HybridSecurityPolicy
  rdf:type :SecurityPolicy ;
  :name "Hybrid Infrastructure Security Policy" ;
  :description "Security policy for hybrid deployments" ;
  :security_type "policy" ;
  :policy_type "network" ;
  :policy_framework "NIST SP 800-53" ;
  :enforcement_mode "enforcing" ;
  :lifecycle_status "active" .

inst:AzureAD_IdP
  rdf:type :IdentityProvider ;
  :name "Azure Active Directory" ;
  :description "Azure AD for unified identity" ;
  :security_type "iam" ;
  :idp_type "saml" ;
  :mfa_enabled true ;
  :lifecycle_status "active" .

inst:OnPremZone
  rdf:type :SecurityZone ;
  :name "On-Premises Trusted Zone" ;
  :description "Trusted zone for on-premises resources" ;
  :security_type "security_zone" ;
  :zone_type "internal" ;
  :trust_level "trusted" ;
  :security_classification "confidential" ;
  :mfa_enabled true ;
  :lifecycle_status "active" .

inst:CloudZone
  rdf:type :SecurityZone ;
  :name "Cloud Analytics Zone" ;
  :description "Zone for cloud analytics services" ;
  :security_type "security_zone" ;
  :zone_type "external" ;
  :trust_level "semi_trusted" ;
  :security_classification "internal" ;
  :mfa_enabled true ;
  :lifecycle_status "active" .

inst:DMZZone
  rdf:type :SecurityZone ;
  :name "Hybrid DMZ Zone" ;
  :description "DMZ for hybrid connectivity" ;
  :security_type "security_zone" ;
  :zone_type "dmz" ;
  :trust_level "semi_trusted" ;
  :security_classification "internal" ;
  :mfa_enabled true ;
  :lifecycle_status "active" .

# Relationships
inst:HybridCertificate :issued_by inst:DigiCertCA .
inst:CloudLoadBalancer :uses_certificate inst:HybridCertificate .
inst:VPNGateway :uses_certificate inst:HybridCertificate .
inst:OnPremFirewall02 :enforces inst:HybridSecurityPolicy .
inst:CloudSecurityGroup03 :enforces inst:HybridSecurityPolicy .
inst:CoreInventoryApp :protected_by inst:OnPremFirewall02 .
inst:InventoryDatabase :protected_by inst:OnPremFirewall02 .
inst:InventoryAnalyticsService :protected_by inst:CloudSecurityGroup03 .
inst:CoreInventoryApp :secured_by inst:HybridSecurityPolicy .
inst:InventoryAnalyticsService :secured_by inst:HybridSecurityPolicy .
inst:CoreInventoryApp :authenticated_by inst:AzureAD_IdP .
inst:InventoryAnalyticsService :authenticated_by inst:AzureAD_IdP .
inst:InventoryAppVM :belongs_to_zone inst:OnPremZone .
inst:InventoryDBVM :belongs_to_zone inst:OnPremZone .
inst:OnPremSubnet :belongs_to_zone inst:OnPremZone .
inst:AnalyticsEC2Instance :belongs_to_zone inst:CloudZone .
inst:CloudSubnet :belongs_to_zone inst:CloudZone .
inst:VPNGateway :belongs_to_zone inst:DMZZone .
inst:AWSTransitGateway :belongs_to_zone inst:DMZZone .
