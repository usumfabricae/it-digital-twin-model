# Relationships
inst:OnPremHypervisor02 :runs_on inst:OnPremPhysicalServer02 .
inst:InventoryAppVM :runs_on inst:OnPremHypervisor02 .
inst:InventoryDBVM :runs_on inst:OnPremHypervisor02 .
inst:JBossAppServer :hosted_on inst:InventoryAppVM .
inst:InventoryDatabase :hosted_on inst:InventoryDBVM .
inst:InventoryDatabase :stored_on inst:InventoryDBVolume .
inst:InventoryDBVolume :allocated_from inst:OnPremStorageArray02 .
inst:InventoryAnalyticsService :hosted_on inst:AnalyticsEC2Instance .
inst:AnalyticsDataWarehouse :hosted_on inst:SnowflakeService .
inst:IntegrationQueue :hosted_on inst:RabbitMQCloudService .

