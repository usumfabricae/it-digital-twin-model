  :lifecycle_status "active" .

# Relationships
inst:VPNGateway :connected_to inst:OnPremCoreSwitch02 .
inst:VPNGateway :connected_to inst:AWSTransitGateway .
inst:CoreInventoryApp :communicates_via inst:OnPremCommPath .
inst:CoreInventoryApp :communicates_via inst:HybridCommPath01 .
inst:InventoryAnalyticsService :communicates_via inst:HybridCommPath01 .
inst:InventoryAnalyticsService :communicates_via inst:HybridCommPath02 .
inst:InventoryAnalyticsService :exposes_via inst:CloudLoadBalancer .
inst:HybridCommPath01 :routes_through inst:VPNGateway .
inst:HybridCommPath01 :routes_through inst:AWSTransitGateway .
inst:HybridCommPath02 :routes_through inst:CloudLoadBalancer .
inst:OnPremCommPath :routes_through inst:OnPremCoreSwitch02 .
inst:CloudLoadBalancer :balances_to inst:AnalyticsEC2Instance .

# ----------------------------------------------------------------------------
# Layer 6: Security Layer (Hybrid Security)
# ----------------------------------------------------------------------------

inst:OnPremFirewall02
