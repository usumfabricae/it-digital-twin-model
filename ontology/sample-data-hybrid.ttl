@prefix : <http://example.org/it-infrastructure-ontology#> .
@prefix inst: <http://example.org/instances#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ============================================================================
# Sample Instance Data: Hybrid Infrastructure
# ============================================================================
# Scenario: Hybrid deployment with on-premises core systems integrated
# with cloud-based analytics and mobile services
# ============================================================================

# ----------------------------------------------------------------------------
# Layer 1: Business Process Layer
# ----------------------------------------------------------------------------

inst:InventoryManagementProcess
  rdf:type :BusinessProcess ;
  :name "Inventory Management Process" ;
  :description "Process for managing product inventory across warehouses" ;
  :owner "Supply Chain Manager" ;
  :criticality "high" ;
  :lifecycle_status "active" ;
  :process_type "core" ;
  :frequency "real-time" ;
  :sla_target "99.5% availability" .

inst:SupplyChainCapability
  rdf:type :BusinessCapability ;
  :name "Supply Chain Management" ;
  :description "Capability to manage end-to-end supply chain" ;
  :owner "VP of Supply Chain" ;
  :criticality "high" ;
  :lifecycle_status "active" ;
  :capability_level 1 ;
  :maturity_level "managed" .

inst:InventorySystemProduct
  rdf:type :Product ;
  :name "Inventory Management System" ;
  :description "Integrated inventory management solution" ;
  :owner "Product Manager" ;
  :criticality "high" ;
  :lifecycle_status "active" ;
  :product_type "software" ;
  :launch_date "2018-09-01"^^xsd:date .

# Relationships
inst:InventoryManagementProcess :part_of inst:SupplyChainCapability .
inst:InventoryManagementProcess :delivers inst:InventorySystemProduct .

# ----------------------------------------------------------------------------
# Layer 2: Application Layer (Hybrid)
# ----------------------------------------------------------------------------

# On-premises core inventory system
inst:CoreInventoryApp
  rdf:type :Application ;
  :name "Core Inventory Application" ;
  :description "Legacy inventory management system" ;
  :version "8.5.2" ;
  :application_type "monolithic" ;
  :deployment_model "vm_based" ;
  :runtime_environment "JBoss EAP 7.4" ;
  :technology_stack "Java EE" ;
  :data_classification "confidential" ;
  :lifecycle_status "production" .

# Cloud-based analytics service
inst:InventoryAnalyticsService
  rdf:type :Service ;
  :name "Inventory Analytics Service" ;
  :description "Cloud-based analytics microservice" ;
  :version "1.3.0" ;
  :application_type "microservice" ;
  :deployment_model "cloud_managed" ;
  :service_type "microservice" ;
  :endpoint_url "https://analytics.inventory.example.com/api"^^xsd:anyURI ;
  :protocol "https" ;
  :lifecycle_status "production" .

# Cloud-based mobile API
inst:MobileInventoryAPI
  rdf:type :API ;
  :name "Mobile Inventory API" ;
  :description "API for mobile inventory applications" ;
  :version "v2" ;
  :api_type "REST" ;
  :endpoint_url "https://mobile-api.inventory.example.com/v2"^^xsd:anyURI ;
  :lifecycle_status "active" .

inst:JBossAppServer
  rdf:type :ApplicationServer ;
  :name "JBoss EAP Server" ;
  :description "JBoss Enterprise Application Platform" ;
  :version "7.4.0" ;
  :application_type "legacy" ;
  :deployment_model "vm_based" ;
  :runtime_environment "JBoss EAP 7.4" ;
  :port 8080 ;
  :lifecycle_status "production" .

# On-premises database
inst:InventoryDatabase
  rdf:type :Database ;
  :name "Inventory Database" ;
  :description "SQL Server database for inventory data" ;
  :version "2019" ;
  :database_type "relational" ;
  :database_product "Microsoft SQL Server" ;
  :data_classification "confidential" ;
  :lifecycle_status "active" .

# Cloud data warehouse
inst:AnalyticsDataWarehouse
  rdf:type :Database ;
  :name "Analytics Data Warehouse" ;
  :description "Snowflake data warehouse for analytics" ;
  :version "7.0" ;
  :database_type "nosql_columnar" ;
  :database_product "Snowflake" ;
  :data_classification "internal" ;
  :lifecycle_status "active" .

# Integration message queue
inst:IntegrationQueue
  rdf:type :MessageQueue ;
  :name "Inventory Integration Queue" ;
  :description "RabbitMQ for on-prem to cloud integration" ;
  :version "3.11" ;
  :protocol "amqp" ;
  :lifecycle_status "active" .

# Relationships
inst:InventoryManagementProcess :realized_by inst:CoreInventoryApp .
inst:InventoryManagementProcess :realized_by inst:InventoryAnalyticsService .
inst:InventorySystemProduct :requires inst:CoreInventoryApp .
inst:InventorySystemProduct :requires inst:InventoryAnalyticsService .
inst:CoreInventoryApp :uses inst:InventoryDatabase .
inst:CoreInventoryApp :uses inst:IntegrationQueue .
inst:InventoryAnalyticsService :uses inst:AnalyticsDataWarehouse .
inst:InventoryAnalyticsService :uses inst:IntegrationQueue .
inst:InventoryAnalyticsService :exposes inst:MobileInventoryAPI .
inst:CoreInventoryApp :deployed_on inst:JBossAppServer .

